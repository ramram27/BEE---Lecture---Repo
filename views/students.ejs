<!DOCTYPE html>
<html>
  <head>
    <title>Students List</title>
    <style>
      table {
        width: 60%;
        border-collapse: collapse;
        margin: 20px 0;
        font-size: 18px;
        text-align: left;
      }
      th,
      td {
        padding: 10px;
        border: 1px solid #ddd;
      }
      th {
        background-color: #f4f4f4;
      }
      tr:nth-child(even) {
        background-color: #f9f9f9;
      }
      h1 {
        font-family: Arial, sans-serif;
      }
    </style>
  </head>
  <body>
    <h1>Students</h1>

    <table>
      <thead>
        <tr>
          <th>#</th>
          <th>Name</th>
          <th>Age</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        <% students.forEach((student, index) => { %>
        <tr>
          <td><%= index + 1 %></td>
          <td><%= student.name %></td>
          <td><%= student.age %></td>
          <td><%= student.email %></td>
        </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
